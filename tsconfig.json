{
    "compilerOptions": {
      "module": "ES6",
      "target": "ES6",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "outDir": "./dist", // 输出目录
      "rootDir": "./app", // 源文件目录
      "skipLibCheck": true // 可选：跳过库文件检查
    },
    "include": ["app/**/*.ts"], // 包含的目录
    "exclude": ["node_modules"] // 排除的目录
  }